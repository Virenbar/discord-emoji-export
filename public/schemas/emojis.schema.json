{
  "$schema": "http://json-schema.org/draft-07/schema",
  "type": "object",
  "properties": {
    "guildName": {
      "title": "Guild Name",
      "type": "string"
    },
    "guildID": {
      "title": "Guild ID",
      "type": "string"
    },
    "emojis": {
      "title": "Array of guild emojis",
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "name": {
            "type": "string"
          },
          "id": {
            "title": "Emoji ID",
            "type": "string"
          },
          "identifier": {
            "title": "Emoji identifier",
            "description": "Format: a?:name:id",
            "type": "string"
          },
          "url": {
            "title": "URL to emoji",
            "type": "string"
          }
        }
      }
    }
  }
}